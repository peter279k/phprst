{
    "name": "petk/phprst",
    "type": "library",
    "description": "reStructuredText PHP library",
    "keywords": ["rst"],
    "homepage": "https://github.com/petk/phprst",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/finder": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "autoload": {
        "psr-0" : {"Rst" : "src/"}
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
